.icon-help:before { content: "\e6a3"; }

.icon-icon-question:before { content: "\e66b"; }

.icon-wenhao:before { content: "\e61f"; }
/* 模拟授权弹框 */
.mask{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 199103
}
.author_view{
  position: fixed;
  z-index: 199104;
  width: 600rpx;
  height: 300rpx;
  background: #fff;
  border-radius: 8rpx;
  margin: 0;
  left: 75rpx;
  top: 300rpx;
}
.author_view .author_tit{
  text-align: center;
  margin-top: 40rpx;
  font-size: 18px;
}
.author_view .author_txt{
  text-align: center;
  color: #666;
  font-size: 14px;
  border-bottom: 1px solid #f1f1f1;
  padding-bottom: 30rpx
}
.author_view button{
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 18px;
  color: #fff;
  background: #ffbe00;
  margin-top: 22rpx
}